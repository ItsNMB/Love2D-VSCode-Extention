{
    "name": "love2d",
    "displayName": "Love2d",
    "description": "Extension for the Love2D game framework.",
    "version": "0.3.6",
    "publisher": "noah",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:lua",
        "onCommand:LOVE.launch",
        "onCommand:LOVE.launchwithdebuglog",
        "onCommand:LOVE.openDocumentation",
        "onCommand:LOVE.openDocumentationInput"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "LOVE.openDocumentation",
                "title": "Open LOVE 2D documentation"
            },
            {
                "command": "LOVE.openDocumentationInput",
                "title": "Open LOVE 2D documentation for input"
            },
            {
                "command": "LOVE.launch",
                "title": "Launch LOVE 2D from the working directory"
            },
            {
                "command": "LOVE.launchwithdebuglog",
                "title": "Launch LOVE 2D from the working directory with debug log"
            }
        ],
        "keybindings": [
            {
                "command": "LOVE.openDocumentation",
                "key": "Ctrl+Alt+D"
            },
            {
                "command": "LOVE.openDocumentationInput",
                "key": "Alt+Shift+I"
            },
            {
                "command": "LOVE.launch",
                "key": "Alt+L"
            },
            {
                "command": "LOVE.launchwithdebuglog",
                "key": "Alt+Shift+L"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "openurl": "^1.1.1",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "openurl": "^1.1.1",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": "https://github.com/bschulte/LOVE-Autocomplete"
}
