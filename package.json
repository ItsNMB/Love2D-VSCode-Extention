{
    "name": "love",
    "displayName": "LOVE",
    "description": "Extension for the LOVE 2D game framework.",
    "version": "0.2.0",
    "publisher": "bschulte",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:lua"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "LOVE.openDocumentation",
                "title": "Open LOVE 2D documentation"
            },
            {
                "command": "LOVE.launch",
                "title": "Launch LOVE 2D from the working directory"
            }
        ],
        "keybindings": [
            {
                "command": "LOVE.openDocumentation",
                "key": "Ctrl+Alt+D"
            },
            {
                "command": "LOVE.launch",
                "key": "Alt+L"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": "https://github.com/bschulte/LOVE-Autocomplete"
}